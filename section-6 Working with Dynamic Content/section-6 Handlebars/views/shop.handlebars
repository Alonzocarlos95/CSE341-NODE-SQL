    <main>
        <div class="grid">
            {{#if hasProducts}}
                {{#each prods}}
                <article class="card product-item">
                    <header class="card_header">
                        <h1 class="product__title">{{this.title}}</h1>
                    </header>
                <div class="card__image">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTJPQxL-l_EPztQDYBsh6Yj_to2mC4BFh61Dw&usqp=CAU" alt="book">

                </div>
                </article>
                {{/each}}
        </div>   
            {{ else}} 
            <h1>No products to shoow</h1>
            {{/if}}
    </main>
